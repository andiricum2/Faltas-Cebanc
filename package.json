{
  "name": "faltas",
  "version": "0.1.0",
  "private": true,
  "main": "src-tauri/src/main.rs",
  "scripts": {
    "start": "next start",
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:tauri\"",
    "dev:next": "next dev --turbopack",
    "dev:tauri": "wait-on http://localhost:3000 && tauri dev -f --config src-tauri/tauri.conf.json",
    "build": "next build",
    "dist": "tauri build -f --config src-tauri/tauri.conf.json",
    "release": "npm run build && tauri build -f --config src-tauri/tauri.conf.json"
  },
  "dependencies": {
    "@tauri-apps/plugin-process": "^2.3.0",
    "@tauri-apps/plugin-updater": "^2.9.0",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.19",
    "lucide-react": "^0.544.0",
    "next": "15.5.4",
    "pino": "^9.5.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "recharts": "^3.2.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "@tauri-apps/cli": "^2.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "wait-on": "^9.0.1"
  }
}
